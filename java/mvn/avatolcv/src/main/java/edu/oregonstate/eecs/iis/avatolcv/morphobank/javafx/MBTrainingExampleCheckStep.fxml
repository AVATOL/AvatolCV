<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox stylesheets="@../../css/javafx.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <Region VBox.vgrow="ALWAYS" />
            <Label text="Here is the training/testing profile for scoring" />
            <ScrollPane VBox.vgrow="ALWAYS">
              <content>
                <AnchorPane>
                     <children>
                        <GridPane fx:id="trainingTestingGridPane" alignment="CENTER" hgap="5.0" vgap="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
                            <ColumnConstraints hgrow="NEVER" minWidth="10.0" />
                              <ColumnConstraints hgrow="NEVER" minWidth="10.0" />
                              <ColumnConstraints hgrow="NEVER" minWidth="10.0" />
                              <ColumnConstraints hgrow="NEVER" minWidth="10.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <ToggleButton mnemonicParsing="false" text="ToggleButton" />
                              <ToggleButton mnemonicParsing="false" text="ToggleButton" GridPane.columnIndex="1" />
                              <Label text="Label that;s long" GridPane.columnIndex="2" />
                              <Label text="Label" GridPane.columnIndex="3" />
                              <Label text="Labele" GridPane.columnIndex="4" />
                           </children>
                        </GridPane>
                     </children>
                  </AnchorPane>
              </content>
            </ScrollPane>
            <Region VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</HBox>
