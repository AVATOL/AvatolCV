<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="400.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="NEVER">
         <children>
            <Label text="Excluding poor quality images" />
            <Label text="Select any image to the right to exclude it" />
            <Label text="Exclude images that are too blurry, like this..." />
            <ImageView fitHeight="60.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
            <Label text="...or poorly contrasted, like this..." />
            <ImageView fitHeight="60.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
            <Label text="...or whose specimen of interest is too damaged, like this..." />
            <ImageView fitHeight="60.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </VBox>
      <ScrollPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
        <content>
          <AnchorPane>
               <children>
                  <VBox fx:id="imageVBox" prefWidth="400.0" spacing="8.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
   </children>
</HBox>
